{
  "expo": {
    "name": "Fisio Evolui",
    "slug": "fisioevolui",
    "scheme": "fisioevolui",
    "version": "1.1.1.11",
  
    "orientation": "portrait",
    "icon": "./assets/icon.png",
  
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#63C2D1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/images/*",
      "src/assets/images/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#63C2D1"
      },
      "versionCode": 4,
      "package": "com.mboldrini.FisioEvolui"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
 
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  },
  "permissions": ["FOREGROUND_SERVICE"]
}
